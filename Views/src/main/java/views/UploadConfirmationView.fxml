<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.UploadConfirmationViewController">
   <center>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="461.0" BorderPane.alignment="CENTER">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Do you want to upload this Business Plan to the Server?">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin>
            </Text>
            <HBox alignment="CENTER" prefHeight="77.0" prefWidth="461.0" spacing="100.0">
               <children>
                  <Button id="confirmButton" alignment="CENTER" mnemonicParsing="false" onAction="#onClickConfirm" text="Confirm" />
                  <Button id="cancelButton" fx:id="closeButton" mnemonicParsing="false" onAction="#onClickCancel" text="Cancel" />
               </children>
            </HBox>
         </children>
      </VBox>
   </center>
</BorderPane>
